<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Avatar.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Avatar.ico" />
    <None Remove="Media\proger.png" />
  </ItemGroup>

  <ItemGroup>
    <Page Remove="Theme.xaml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FontAwesome5" Version="2.1.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Avatar.ico" />
    <Resource Include="Media\proger.png" />
    <Resource Include="Theme.xaml">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="WindowSuccessSend - Копировать.xaml.cs">
      <SubType>Code</SubType>
      <DependentUpon>WindowSuccessSend.xaml.cs</DependentUpon>
    </Compile>
  </ItemGroup>

</Project>
